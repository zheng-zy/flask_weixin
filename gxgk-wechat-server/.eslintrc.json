{
  "rules": {
    "indent": [
      2,
      4
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": 0,
    "eqeqeq": 2
  },
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "wx": true
  },
  "extends": "eslint:recommended"
}
